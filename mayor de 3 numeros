cout<<"Ingrese a:";
cin>>a;
cout<<"Ingrese b:";
cin>>b;
cout<<"Ingrese c:";
cin>>c;

if(a>b)
  if(a>c)
	  cout<<"EL MAYOR ES:"<<a;
  else
	  cout<<"EL MEYOR ES:"<<c;
else
if (b>c)
cout<<"EL MAYOR ES:"<<b;
else 
cout<<"EL MAYOR ES:"<<c;


getch();

}
