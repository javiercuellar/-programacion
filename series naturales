void main()

{
  int suma,i,N;
  cout<<"INGRESAR EL VALOR DE N";
  cin>> N;
  suma=0;
  for(i=1;i<=N;i++)
suma=suma+i;
  cout<< "LA suma DE LOS "<< N <<" PRIMEROS NATURALES ES: " << suma;
  getch();


